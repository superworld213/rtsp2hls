{
  "name": "rtsp2hls",
  "private": true,
  "version": "0.0.0",
  "description": "RTSP to HLS converter desktop application",
  "author": "RTSP2HLS Team",
  "type": "module",
  "main": "electron/main.cjs",
  "homepage": "./",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "dev": "concurrently \"vite\" ",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc -b --noEmit",
    "electron": "electron electron/main.cjs",
    "electron-dev": "chcp 65001 && cross-env NODE_ENV=development electron electron/main.cjs",
    "electron-build": "npm run build && electron-builder",
    "dist": "npm run build && electron-builder --config electron.config.json",
    "install:cnpm": "cnpm install",
    "update:cnpm": "cnpm update"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "antd": "^5.26.6",
    "async": "^3.2.6",
    "child_process": "^1.0.2",
    "clsx": "^2.1.1",
    "electron-log": "^5.4.1",
    "fluent-ffmpeg": "^2.1.3",
    "hls.js": "^1.6.7",
    "isexe": "^3.1.1",
    "lucide-react": "^0.511.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.3.0",
    "sonner": "^2.0.2",
    "tailwind-merge": "^3.0.2",
    "which": "^5.0.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/hls.js": "^1.0.0",
    "@types/node": "^22.16.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-dev-locator": "^1.0.0",
    "cross-env": "^7.0.3",
    "electron": "^25.3.0",
    "electron-builder": "^24.6.4",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
